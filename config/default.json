{
  "providers": {
    "ai": {
      "default": "openai",
      "azure-openai": {
        "type": "azure-openai",
        "endpoint": "https://test.openai.azure.com",
        "deploymentName": "gpt-4"
      },
      "openai": {
        "type": "openai",
        "endpoint": "https://api.openai.com/v1",
        "apiKey": "test-key"
      }
    },
    "dataSources": {
      "default": "application-insights",
      "application-insights": {
        "type": "application-insights",
        "applicationId": "test-app-id",
        "tenantId": "test-tenant",
        "endpoint": "https://api.applicationinsights.io/v1/apps"
      },
      "log-analytics": {
        "type": "log-analytics",
        "workspaceId": "test-workspace",
        "tenantId": "test-tenant",
        "endpoint": "https://api.loganalytics.io/v1/workspaces"
      }
    },
    "auth": {
      "default": "azure-managed-identity",
      "azure-managed-identity": {
        "type": "azure-managed-identity",
        "tenantId": "test-tenant"
      }
    }
  },
  "logLevel": "debug",
  "language": "auto",
  "fallbackBehavior": {
    "enableProviderFallback": true,
    "aiProviderOrder": ["openai", "azure-openai"],
    "dataSourceProviderOrder": ["application-insights", "log-analytics"]
  }
}
